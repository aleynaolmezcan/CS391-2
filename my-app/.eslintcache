/my-app/src/index.js",[],["152","153"],"/home/saydemr/CS391-2/my-app/src/App.js",["154"],"import './App.css';\nimport {\n  BrowserRouter as Router,\n  Route\n} from \"react-router-dom\";\nimport React, { Component } from 'react';\n\n\nimport Abdullah from './Components/AbdullahPage/Abdullah';\nimport Home from './Components/HomePage/Home';\nimport Aleyna from './Components/AleynaPage/Aleyna';\nimport Burcu from './Components/BurcuPage/Burcu';\nimport Ozge from  './Components/OzgePage/Ozge';\nimport Esad from \"./Components/EsadPage/Esad\";\nimport Esma from \"./Components/EsmaHocaPage/Esma\";\n\n\n\nfunction App() {\n  return (\n\n    <Router>\n      <Route exact path=\"/\" component = {Home}>\n        <Home/>\n      </Route>\n      <Route exact path=\"/Abdullah\" component = {Abdullah}>\n        <Abdullah/>  \n      </Route>\n      <Route exact path=\"/Aleyna\" component = {Aleyna}>\n        <Aleyna/>  \n        </Route>\n        <Route exact path=\"/Burcu\" component = {Burcu}>\n        <Burcu/>  \n        </Route>\n        <Route exact path=\"/Esad\" component = {Esad}>\n        <Esad/>  \n        </Route>\n        <Route exact path=\"/Ozge\" component = {Ozge}>\n        <Ozge/>  \n        </Route>\n        <Route exact path=\"/Esma\" component = {Esma}>\n        <Esma/>  \n        </Route>\n    </Router>\n    \n\n\n\n\n    \n     \n    \n\n   \n    \n    \n\n \n    \n\n   \n        \n   \n      \n      \n    \n\n    \n    \n\n   \n    \n    \n    \n  \n  );\n}\n\nexport default App;\n","/home/saydemr/CS391-2/my-app/src/reportWebVitals.js",[],"/home/saydemr/CS391-2/my-app/src/Components/AbdullahPage/Abdullah.js",[],"/home/saydemr/CS391-2/my-app/src/Components/AleynaPage/Aleyna.js",[],"/home/saydemr/CS391-2/my-app/src/Components/BurcuPage/Burcu.js",[],"/home/saydemr/CS391-2/my-app/src/Components/EsadPage/Esad.js",[],"/home/saydemr/CS391-2/my-app/src/Components/HomePage/Home.js",["155"],"import React from 'react';\r\nimport HomeHeader from './HomeHeader.js';\r\nimport Table from './Table.js';\r\nimport HomeFooter from './HomeFooter.js';\r\n\r\nimport backGroundImage from '../../images/background.png';\r\n\r\nimport './Home.css'\r\n\r\n class Home extends React.Component{\r\n\r\n  componentDidMount() {\r\n    document.body.style.backgroundImage = \"('../../images/background.png')\";\r\n  }  \r\n\r\n\r\n\r\n\r\n    render(){\r\n         return <body>\r\n                      <header>\r\n                      <HomeHeader/>\r\n                      </header>\r\n                      <Table/>\r\n                      <footer>\r\n                      <HomeFooter/>\r\n                      </footer>     \r\n                </body>\r\n    }\r\n}\r\n\r\nexport default Home;","/home/saydemr/CS391-2/my-app/src/Components/EsmaHocaPage/Esma.js",[],"/home/saydemr/CS391-2/my-app/src/Components/OzgePage/Ozge.js",["156"],"import React from 'react';\r\nimport OzgeHeader from  './OzgeHeader';\r\nimport OzgeBody from  './OzgeBody';\r\nimport OzgeForm from './OzgeForm';\r\nimport OzgeFooter from './OzgeFooter';\r\n\r\nexport default class Ozge extends React.Component{\r\n\r\n    componentDidMount() {\r\n        document.body.style.backgroundColor = \"#000000\";\r\n      }\r\n\r\n\r\n    render(){\r\n        return <body>\r\n                    <header>\r\n                    <OzgeHeader/>\r\n                    </header>\r\n                    <OzgeBody/>\r\n                    \r\n                    <footer>\r\n                    <OzgeFooter/>\r\n                    </footer>\r\n                </body>\r\n    }\r\n}","/home/saydemr/CS391-2/my-app/src/Components/AbdullahPage/AbdullahHeader.js",[],"/home/saydemr/CS391-2/my-app/src/Components/HomePage/HomeHeader.js",["157"],"import React from 'react';\r\nimport './HomeHeader.css';\r\nimport headerLogo from '../../images/home_header.png';\r\n\r\nclass HomeHeader extends React.Component{\r\n    render(){\r\n        return <div id = \"headerimg\">\r\n                    <img src = {headerLogo} ></img>\r\n                </div> \r\n    }\r\n    \r\n}\r\n\r\nexport default HomeHeader;","/home/saydemr/CS391-2/my-app/src/Components/HomePage/HomeFooter.js",[],"/home/saydemr/CS391-2/my-app/src/Components/AbdullahPage/AbdullahFooter.js",[],"/home/saydemr/CS391-2/my-app/src/Components/AbdullahPage/AbdullahBody.js",["158"],"import React from 'react';\nimport './AbdullahBody.css';\nimport logo from '../../images/abdullah.jpg'; // with import\n\nclass AbdullahBody extends React.Component{\n    render(){\n        return <div id = \"abdüBody\"> \n                    <br/><br/><br/><br/><br/><br/>\n                    <img src={logo} id = \"abdü\" alt=\"Abdullah Saydemir\" width=\"200\" height=\"200\"></img>\n                    <article>\n\n                        <h1>Abdullah Saydemir</h1>\n\n                        <h2>Some do it with a bitter look,\n                        Some with a flattering word\n                        </h2>\n\n                            <p id = \"abdüP\">\n\n                            I don't have much to say about myself <b><b>but</b>\n                            </b> there is an appalling story about three lizards.\n                            Originally the story is written in Turkish, so I will continue Turkish.\n                            If I did not know Turkish, I would learn just to understand this. <br/><br/>\n\n\n                            Zamanın birinde 3 tane kertenkele varmış. Bunlar baya iyi kardeşlermiş. Yedikleri içtikleri ayrı\n                            gitmezmiş. Bunların ağzına kadar dolu bir yemek depoları ve lükse kaçmayan ama ihtiyaçlarına da\n                            cevap veren bir evleri varmış. Bunların tek kuralları evden çıkış ve eve geliş sıralarıymış. Evden\n                            ilk küçük çıkar ve en son da eve o gelirmiş.\n                            Günün birinde bir ejderha bunlar evde yokken evlerine gelmiş, yemeklerinden çalabildiklerini çalmış\n                            sonra kalanları yakmış, evi de yıkıp gitmiş. Akşama doğru eve gelen büyük kertenkele evi ve depoyu\n                            görüp çok sinirlenmiş, ejderhanın ayak izlerini takip etmiş. Ejderhaya yaklaşırken ejderha onun\n                            geldiğini\n                            fark etmiş ve bir kuyruk darbesiyle onu öldürmüş. Büyük ejderhanın cesedi savrulup kül olan evin\n                            yakınlarına\n                            düşmüş.<br/><br/>\n\n                            Daha sonra ortanca eve gelmiş, bakmış abisi ölmüş, ev yanıyor depodaki yemekler de çalınmış. Çok\n                            sinirlenmiş. Sinirle o da ejderhanın ayak izlerini takip etmiş. Büyük kertenkeleden daha fazla yaklaşmış\n                            ejderhaya\n                            fakat\n                            o da başaramamış. Ejderha bir alevle onu küle çevirmiş. Ortancanın sadece iskeleti kalmış ortada.<br/><br/>\n                        \n                            Küçük eve gelmiş. Bakmış abisi ölü, ortanca yok, ev yanıyor, depo bomboş. O da sinirlenmiş fakat\n                            olanları\n                            anlayıp sinirle hareket etmemiş. Gece olana kadar bir yer bulmuş ve uzunca ne yapacağını düşünmüş.\n                            Planını\n                            uygulamak için yanına <b><i>sivri bir kurşun kalem, boyunun yarısı kadar bir bez parçası</i></b> ve\n                            <b><i> dayanıklı\n                                    bir urgan</i></b> almış ve yola koyulmuş. Yolda yürürken ortancanın iskeletini görmüş, birkaç\n                            saniye gözyaşı\n                            dökmüş ama intikam alacağını düşünüp bu duyguyu hemen savuşturmuş. Uzaktan ejderhayı görmüş. Ejderha\n                            büyük\n                            bir mağaranın içinde sırtı taşa gelecek şekilde yatıyormuş.<br/><br/>\n                        \n                            Küçük kertenkele iyice yaklaşmış fakat sonra ses çıkarabileceği aklına gelmiş. Bezden biraz koparmış ve\n                            ayaklarını\n                            o bezle güzelce kaplamış bu sayede yürürken artık ses çıkmıyormuş. Sonra mağaranın içine girmiş.\n                            Ejderhanın arkasına\n                            geçmek için duvardan yürümüş. Arkasına geçince eline sivri kurşun kalemi almış. Sonra ejderha onu da\n                            görmüş ve öldürmüş.<br/><br/>\n                            </p>\n                    </article>\n                    <a href=\"https://github.com/Saydemr\" id = \"github\" class=\"fa fa-github fa-3x\" ></a>\n                    <br/><br/>               \n                </div>\n                \n\n    }\n}\n\nexport default AbdullahBody;","/home/saydemr/CS391-2/my-app/src/Components/AbdullahPage/AbdullahForm.js",[],"/home/saydemr/CS391-2/my-app/src/Components/HomePage/Table.js",[],"/home/saydemr/CS391-2/my-app/src/Components/AleynaPage/AleynaHeader.js",[],"/home/saydemr/CS391-2/my-app/src/Components/AleynaPage/AleynaFooter.js",[],"/home/saydemr/CS391-2/my-app/src/Components/BurcuPage/BurcuHeader.js",[],"/home/saydemr/CS391-2/my-app/src/Components/EsadPage/EsadBody.js",["159","160","161"],"import React from 'react';\r\nimport './EsadBody.css';\r\nimport logo from '../../images/esad.jpg';\r\n\r\nclass EsadBody extends React.Component {\r\n    render() {\r\n        return <div id=\"esadBody\">\r\n        \r\n                <br/><br/><br/><br/><br/><br/>\r\n\r\n                    <img src = {logo} alt= \"Fotoğraf\"/>\r\n                        <article>\r\n                            <h1>Muhammed Esad Simitcioglu</h1>\r\n                            <article>\r\n                                <h2>09 October 1999</h2>\r\n                                <p id = \"esadP\">Hello there.My name is Esad Simitcioglu.I am a third year computer engineer student.</p>\r\n                                <br/>\r\n                                <a href=\"htt